        "liquid": "<!-- POPUP BIENVENUE -10% -->\n<style>\n  .popup-overlay {\n    position: fixed;\n    inset: 0;\n    background: rgba(0,0,0,.55);\n    display: none;\n    align-items: center;\n    justify-content: center;\n    z-index: 99999;\n    padding: 16px;\n  }\n  .popup-card {\n    width: min(520px, 100%);\n    background: #fff;\n    border-radius: 18px;\n    box-shadow: 0 20px 60px rgba(0,0,0,.25);\n    overflow: hidden;\n    transform: translateY(10px);\n    opacity: 0;\n    transition: .18s ease;\n    font-family: system-ui, -apple-system, Segoe UI, Roboto, Arial, sans-serif;\n  }\n  .popup-overlay.show .popup-card {\n    transform: translateY(0);\n    opacity: 1;\n  }\n  .popup-header {\n    padding: 18px 18px 0 18px;\n    display: flex;\n    justify-content: space-between;\n    align-items: flex-start;\n    gap: 12px;\n  }\n  .popup-close {\n    border: 0;\n    background: transparent;\n    font-size: 22px;\n    line-height: 1;\n    cursor: pointer;\n    padding: 6px 8px;\n    border-radius: 10px;\n  }\n  .popup-close:hover { background: #f3f3f3; }\n\n  .popup-body {\n    padding: 0 18px 18px 18px;\n  }\n  .popup-title {\n    margin: 0;\n    font-size: 22px;\n    font-weight: 800;\n  }\n  .popup-subtitle {\n    margin: 10px 0 0 0;\n    color: #444;\n    line-height: 1.45;\n    font-size: 15px;\n  }\n  .coupon {\n    margin: 14px 0 0 0;\n    display: flex;\n    gap: 10px;\n    align-items: center;\n    background: #fafafa;\n    border: 1px dashed #cfcfcf;\n    border-radius: 14px;\n    padding: 12px;\n  }\n  .coupon code {\n    font-size: 16px;\n    font-weight: 800;\n    letter-spacing: .8px;\n  }\n  .btn {\n    display: inline-flex;\n    align-items: center;\n    justify-content: center;\n    gap: 8px;\n    border: 0;\n    cursor: pointer;\n    border-radius: 14px;\n    padding: 12px 14px;\n    font-weight: 700;\n    font-size: 14px;\n    transition: .15s ease;\n    user-select: none;\n  }\n  .btn-primary {\n    background: #111;\n    color: #fff;\n  }\n  .btn-primary:hover { transform: translateY(-1px); }\n  .btn-ghost {\n    background: #f3f3f3;\n    color: #111;\n  }\n  .btn-ghost:hover { background: #eaeaea; }\n  .popup-actions {\n    margin-top: 14px;\n    display: flex;\n    gap: 10px;\n    flex-wrap: wrap;\n  }\n  .small {\n    margin-top: 10px;\n    color: #666;\n    font-size: 12px;\n  }\n</style>\n\n<div class=\"popup-overlay\" id=\"welcomePopup\" aria-hidden=\"true\">\n  <div class=\"popup-card\" role=\"dialog\" aria-modal=\"true\" aria-labelledby=\"popupTitle\">\n    <div class=\"popup-header\">\n      <h2 class=\"popup-title\" id=\"popupTitle\">Bienvenue üéâ</h2>\n      <button class=\"popup-close\" id=\"popupClose\" aria-label=\"Fermer\">√ó</button>\n    </div>\n\n    <div class=\"popup-body\">\n      <p class=\"popup-subtitle\">\n        Profite de <strong>-10%</strong> sur ta premi√®re commande.\n      </p>\n\n      <div class=\"coupon\">\n        <div style=\"flex:1\">\n          Code promo : <code id=\"couponCode\">BIENVENUE10</code>\n        </div>\n        <button class=\"btn btn-ghost\" id=\"copyBtn\" type=\"button\">Copier</button>\n      </div>\n\n      <div class=\"popup-actions\">\n        <button class=\"btn btn-primary\" id=\"shopNow\" type=\"button\">J‚Äôen profite</button>\n        <button class=\"btn btn-ghost\" id=\"later\" type=\"button\">Plus tard</button>\n      </div>\n\n      <div class=\"small\">\n        *Valable une seule fois. Non cumulable avec d‚Äôautres offres.\n      </div>\n    </div>\n  </div>\n</div>\n\n<script>\n  (function () {\n    const POPUP_ID = \"welcomePopup\";\n    const STORAGE_KEY = \"welcome_popup_seen_v1\";\n    const DELAY_MS = 1200; // d√©lai avant affichage\n    const popup = document.getElementById(POPUP_ID);\n    const closeBtn = document.getElementById(\"popupClose\");\n    const laterBtn = document.getElementById(\"later\");\n    const shopNowBtn = document.getElementById(\"shopNow\");\n    const copyBtn = document.getElementById(\"copyBtn\");\n    const couponCode = document.getElementById(\"couponCode\").innerText;\n\n    function openPopup() {\n      popup.style.display = \"flex\";\n      requestAnimationFrame(() => popup.classList.add(\"show\"));\n      popup.setAttribute(\"aria-hidden\", \"false\");\n    }\n\n    function closePopup() {\n      popup.classList.remove(\"show\");\n      setTimeout(() => (popup.style.display = \"none\"), 150);\n      popup.setAttribute(\"aria-hidden\", \"true\");\n      localStorage.setItem(STORAGE_KEY, \"1\");\n    }\n\n    // Affiche une seule fois par navigateur\n    const alreadySeen = localStorage.getItem(STORAGE_KEY) === \"1\";\n    if (!alreadySeen) {\n      setTimeout(openPopup, DELAY_MS);\n    }\n\n    // Actions\n    closeBtn.addEventListener(\"click\", closePopup);\n    laterBtn.addEventListener(\"click\", closePopup);\n\n    // Clique en dehors = fermer\n    popup.addEventListener(\"click\", (e) => {\n      if (e.target === popup) closePopup();\n    });\n\n    // √âchap = fermer\n    document.addEventListener(\"keydown\", (e) => {\n      if (e.key === \"Escape\" && popup.style.display === \"flex\") closePopup();\n    });\n\n    // Copier le code\n    copyBtn.addEventListener(\"click\", async () => {\n      try {\n        await navigator.clipboard.writeText(couponCode);\n        copyBtn.textContent = \"Copi√© ‚úÖ\";\n        setTimeout(() => (copyBtn.textContent = \"Copier\"), 1200);\n      } catch (err) {\n        // fallback simple\n        const ta = document.createElement(\"textarea\");\n        ta.value = couponCode;\n        document.body.appendChild(ta);\n        ta.select();\n        document.execCommand(\"copy\");\n        document.body.removeChild(ta);\n        copyBtn.textContent = \"Copi√© ‚úÖ\";\n        setTimeout(() => (copyBtn.textContent = \"Copier\"), 1200);\n      }\n    });\n\n    // Bouton \"J‚Äôen profite\" -> exemple: redirige vers /collections/all\n    shopNowBtn.addEventListener(\"click\", () => {\n      closePopup();\n      window.location.href = \"/collections/all\";\n    });\n  })();\n</script>",
