round:rgba(0,0,0,.08);border:0;margin:14px 0 16px;}\n\n  .elxR-bullets{list-style:none;padding:0;margin:0;display:grid;gap:10px;}\n  .elxR-bullets li{display:flex;align-items:flex-start;gap:10px;font-size:14px;color:#222;}\n  .elxR-check svg{width:18px;height:18px;display:block;}\n\n  /* Fade premium */\n  .elxR-quote.is-fading .avatar,\n  .elxR-quote.is-fading .qtext{transition:opacity .35s ease;opacity:0;}\n  .elxR-quote.is-show .avatar,\n  .elxR-quote.is-show .qtext{transition:opacity .35s ease;opacity:1;}\n\n  @media (max-width:640px){\n    .elxR-title{font-size:34px;}\n    .elxR-dots{position:static;transform:none;margin-left:8px;}\n    .elxR-quote{align-items:flex-start;}\n  }\n</style>\n\n<section class=\"elxR-banner\" data-elx-reviews>\n  <div class=\"elxR-toprow\">\n    <div class=\"elxR-reviews-link\">\n      <span class=\"elxR-stars\">\n        <span>‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ</span>\n        <span class=\"elxR-rating\">4,8</span>\n      </span>\n      <a href=\"#avis\" aria-label=\"Voir les avis\">3269 avis</a>\n    </div>\n\n    <div class=\"elxR-flag\">\n      <span style=\"font-size:18px;\">üá´üá∑</span>\n      <span>Marque fran√ßaise</span>\n    </div>\n  </div>\n\n  <div class=\"elxR-quote\">\n    <img class=\"avatar\" data-elx-avatar src=\"https://i.pravatar.cc/84?img=28\" alt=\"Avatar cliente\">\n    <div class=\"qtext\">\n      <p class=\"qtitle\" data-elx-quote>Ultra pr√©cis sur la l√®vre et le menton. Z√©ro irritation.</p>\n      <p class=\"qmeta\" data-elx-meta>Sophie L. ¬∑ Lyon, France ¬∑ il y a 12 jours (30/01/2026)</p>\n    </div>\n    <div class=\"elxR-dots\" aria-hidden=\"true\">\n      <span class=\"elxR-dot active\" data-idx=\"0\"></span>\n      <span class=\"elxR-dot\" data-idx=\"1\"></span>\n      <span class=\"elxR-dot\" data-idx=\"2\"></span>\n    </div>\n  </div>\n\n  <h1 class=\"elxR-title\">ELINEA+ ¬∑ √âpilateur Visage Haute Pr√©cision</h1>\n  <hr class=\"elxR-hr\">\n\n  <ul class=\"elxR-bullets\">\n    <li>\n      <span class=\"elxR-check\" aria-hidden=\"true\">\n        <svg viewBox=\"0 0 24 24\" fill=\"none\">\n          <path d=\"M20 6L9 17l-5-5\" stroke=\"#111\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n        </svg>\n      </span>\n      <span><strong>√âpilation douce</strong> : confortable, sans irritation</span>\n    </li>\n\n    <li>\n      <span class=\"elxR-check\" aria-hidden=\"true\">\n        <svg viewBox=\"0 0 24 24\" fill=\"none\">\n          <path d=\"M20 6L9 17l-5-5\" stroke=\"#111\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n        </svg>\n      </span>\n      <span><strong>Ultra pr√©cis</strong> : menton, l√®vre, joues, sourcils</span>\n    </li>\n\n    <li>\n      <span class=\"elxR-check\" aria-hidden=\"true\">\n        <svg viewBox=\"0 0 24 24\" fill=\"none\">\n          <path d=\"M20 6L9 17l-5-5\" stroke=\"#111\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n        </svg>\n      </span>\n      <span><strong>Peau lisse</strong> : fini le duvet, repousse plus discr√®te</span>\n    </li>\n  </ul>\n</section>\n\n<script>\n(function(){\n  function initOne(section){\n    const quoteWrap = section.querySelector('.elxR-quote');\n    const quoteEl  = section.querySelector('[data-elx-quote]');\n    const metaEl   = section.querySelector('[data-elx-meta]');\n    const avatarEl = section.querySelector('[data-elx-avatar]');\n    const dots     = Array.from(section.querySelectorAll('.elxR-dot'));\n    if(!quoteWrap || !quoteEl || !metaEl || !avatarEl || dots.length === 0) return;\n\n    const cities = [\n      \"Paris\",\"Lyon\",\"Marseille\",\"Toulouse\",\"Nice\",\"Nantes\",\"Strasbourg\",\"Montpellier\",\n      \"Bordeaux\",\"Lille\",\"Rennes\",\"Grenoble\",\"Reims\",\"Dijon\",\"Angers\",\"Aix-en-Provence\"\n    ];\n\n    // Avis ELINEA+ (visage)\n    const slides = [\n      { quote:\"Ultra pr√©cis sur la l√®vre et le menton. Z√©ro irritation.\", name:\"Sophie L.\",  avatar:\"https://i.pravatar.cc/84?img=28\", daysAgo: 12 },\n      { quote:\"Rapide le matin, peau nette toute la journ√©e. J‚Äôadore.\",    name:\"Camille D.\", avatar:\"https://i.pravatar.cc/84?img=15\", daysAgo: 6  },\n      { quote:\"Tr√®s doux sur les joues, et la repousse est plus discr√®te.\", name:\"√âmilie R.\",  avatar:\"https://i.pravatar.cc/84?img=45\", daysAgo: 3  }\n    ];\n\n    /* R√©glages */\n    const AUTOPLAY_MS = 3500;\n    const FADE_MS = 350;\n    const SWIPE_THRESHOLD = 35;\n\n    // Emp√™che double init (Shopify section reload)\n    if(section.dataset.elxInit === \"1\") return;\n    section.dataset.elxInit = \"1\";\n\n    let current = 0;\n    let timer = null;\n    let isPaused = false;\n    let isAnimating = false;\n\n    function fmtDate(daysAgo){\n      const d = new Date(Date.now() - (daysAgo * 86400000));\n      const dd = String(d.getDate()).padStart(2,\"0\");\n      const mm = String(d.getMonth()+1).padStart(2,\"0\");\n      const yyyy = d.getFullYear();\n      return `${dd}/${mm}/${yyyy}`;\n    }\n\n    function setDots(idx){\n      dots.forEach((d,i)=> d.classList.toggle(\"active\", i===idx));\n    }\n\n    function paint(idx){\n      const s = slides[idx];\n      quoteEl.textContent = s.quote;\n      avatarEl.src = s.avatar;\n      metaEl.textContent = `${s.name} ¬∑ ${cities[idx % cities.length]}, France ¬∑ il y a ${s.daysAgo} jours (${fmtDate(s.daysAgo)})`;\n      setDots(idx);\n      current = idx;\n    }\n\n    function render(idx){\n      if(isAnimating) return;\n      isAnimating = true;\n\n      quoteWrap.classList.remove('is-show');\n      quoteWrap.classList.add('is-fading');\n\n      setTimeout(function(){\n        paint(idx);\n\n        quoteWrap.classList.remove('is-fading');\n        quoteWrap.classList.add('is-show');\n\n        setTimeout(function(){\n          isAnimating = false;\n        }, FADE_MS);\n      }, FADE_MS);\n    }\n\n    function next(){ render((current + 1) % slides.length); }\n    function prev(){ render((current - 1 + slides.length) % slides.length); }\n\n    function stopAuto(){\n      clearInterval(timer);\n      timer = null;\n    }\n\n    function startAuto(){\n      stopAuto();\n      if(isPaused) return;\n      timer = setInterval(next, AUTOPLAY_MS);\n    }\n\n    function resetAuto(){\n      startAuto();\n    }\n\n    // Init\n    quoteWrap.classList.add('is-show');\n    paint(0);\n    startAuto();\n\n    // Dots\n    dots.forEach(dot=>{\n      dot.addEventListener(\"click\", ()=>{\n        const idx = parseInt(dot.getAttribute(\"data-idx\"), 10);\n        if(!Number.isFinite(idx) || idx === current) return;\n        render(idx);\n        resetAuto();\n      });\n    });\n\n    // Pause hover (desktop)\n    section.addEventListener(\"mouseenter\", function(){\n      isPaused = true;\n      stopAuto();\n    });\n    section.addEventListener(\"mouseleave\", function(){\n      isPaused = false;\n      startAuto();\n    });\n\n    // Swipe mobile\n    let touchStartX = 0;\n    let touchStartY = 0;\n    let touchActive = false;\n\n    quoteWrap.addEventListener(\"touchstart\", function(e){\n      if(!e.touches || !e.touches[0]) return;\n      touchActive = true;\n      touchStartX = e.touches[0].clientX;\n      touchStartY = e.touches[0].clientY;\n      isPaused = true;\n      stopAuto();\n    }, {passive:true});\n\n    quoteWrap.addEventListener(\"touchmove\", function(e){\n      if(!touchActive || !e.touches || !e.touches[0]) return;\n      const dx = e.touches[0].clientX - touchStartX;\n      const dy = e.touches[0].clientY - touchStartY;\n      if(Math.abs(dy) > Math.abs(dx) + 10){\n        touchActive = false; // laisse le scroll vertical\n      }\n    }, {passive:true});\n\n    quoteWrap.addEventListener(\"touchend\", function(e){\n      if(!touchActive){\n        isPaused = false;\n        startAuto();\n        return;\n      }\n      touchActive = false;\n\n      const endX = (e.changedTouches && e.changedTouches[0]) ? e.changedTouches[0].clientX : touchStartX;\n      const dx = endX - touchStartX;\n\n      if(Math.abs(dx) >= SWIPE_THRESHOLD){\n        if(dx < 0) next(); else prev();\n      }\n\n      isPaused = false;\n      resetAuto();\n    }, {passive:true});\n  }\n\n  function initAll(){\n    document.querySelectorAll('[data-elx-reviews]').forEach(initOne);\n  }\n\n  if (document.readyState === \"loading\") {\n    document.addEventListener(\"DOMContentLoaded\", initAll);\n  } else {\n    initAll();\n  }\n\n  // Shopify Theme Editor support\n  document.addEventListener(\"shopify:section:load\", initAll);\n})();\n</script>"
